  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Incoverse/IRIS/issues"
  },
  "homepage": "https://github.com/Incoverse/IRIS#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "convert-svg-to-png": "latest",
    "discord.js": "^14.14.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "join-images": "^1.1.5",
    "jsonc-parser": "^3.2.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
